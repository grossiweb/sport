{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "_id",
    "active",
    "age",
    "birth_place_city",
    "birth_place_country",
    "createdAt",
    "date_of_birth",
    "display_height",
    "display_name",
    "display_weight",
    "experience_years",
    "first_name",
    "height",
    "id",
    "jersey",
    "last_name",
    "meta",
    "position",
    "position_abbreviation",
    "slug",
    "sport_id",
    "status",
    "team_id",
    "updated_at",
    "updatedAt",
    "weight"
  ],
  "properties": {
    "_id": {
      "$ref": "#/$defs/ObjectId"
    },
    "active": {
      "type": "boolean"
    },
    "age": {
      "type": "integer"
    },
    "birth_place_city": {
      "type": "string"
    },
    "birth_place_country": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "date_of_birth": {
      "type": "string"
    },
    "display_height": {
      "type": "string"
    },
    "display_name": {
      "type": "string"
    },
    "display_weight": {
      "type": "string"
    },
    "experience_years": {
      "type": "integer"
    },
    "first_name": {
      "type": "string"
    },
    "height": {
      "type": "integer"
    },
    "id": {
      "type": "integer"
    },
    "jersey": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "meta": {
      "type": "object",
      "required": [
        "fetchedAt",
        "sourceUrl"
      ],
      "properties": {
        "fetchedAt": {
          "type": "string"
        },
        "sourceUrl": {
          "type": "string"
        }
      }
    },
    "position": {
      "type": "string"
    },
    "position_abbreviation": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "sport_id": {
      "type": "integer"
    },
    "status": {
      "type": "string"
    },
    "team_id": {
      "type": "integer"
    },
    "updated_at": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string"
    },
    "weight": {
      "type": "integer"
    }
  },
  "$defs": {
    "ObjectId": {
      "type": "object",
      "properties": {
        "$oid": {
          "type": "string",
          "pattern": "^[0-9a-fA-F]{24}$"
        }
      },
      "required": [
        "$oid"
      ],
      "additionalProperties": false
    }
  }
}